{
    "name": "vscode-lbnf",
    "displayName": "LBNF",
    "description": "An extension for VS Code which provides support for the LBNF language.",
    "version": "1.0.3",
    "publisher": "agurodriguez",
    "repository": {
        "type": "git",
        "url": "https://github.com/agurodriguez/vscode-lbnf.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lbnf",
            "aliases": ["LBNF", "lbnf"],
            "extensions": ["cf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lbnf",
            "scopeName": "source.lbnf",
            "path": "./syntaxes/lbnf.tmLanguage.json"
        }]
    },
    "icon": "docs/icon.png",
    "keywords": [
        "bnf", "bnfc", "lbnf", "syntax-highlighting"
    ]
}